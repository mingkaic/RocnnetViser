  "scripts": {
    "start": "tsc && electron main/main.js",
    "reinstall": "npm install && ./node_modules/.bin/electron-rebuild",
    "//": "",
    "electro_build": "HOME=~/.electron-gyp node-gyp rebuild --target=1.4.12 --arch=x64 --dist-url=https://atom.io/download/electron",
    "cclean": "rm -rf build",
    "cbuild": "npm run node-gyp configure build && npm run ctype && npm run electro_build",
    "ctype": "npm run ndts . > temp.txt && sed 1,4d temp.txt > viser_api/rocnnet-types.d.ts && rm temp.txt",
    "autogypi": "autogypi",
    "node-gyp": "node-gyp",
    "emcc-path": "emcc-path",
    "copyasm": "copyasm",
    "ndts": "ndts"
  },
  "repository": "",
  "keywords": [
    "Electron",
    "Rocnnet",
    "demo"
  ],
  "author": "mingkaic",
